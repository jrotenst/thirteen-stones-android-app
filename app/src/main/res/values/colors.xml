<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="primaryColor">#80cbc4</color>
    <color name="primaryLightColor">#b2fef7</color>
    <color name="primaryDarkColor">#4f9a94</color>
    <color name="secondaryColor">#006064</color>
    <color name="secondaryLightColor">#428e92</color>
    <color name="secondaryDarkColor">#00363a</color>
    <color name="primaryTextColor">#000000</color>
    <color name="secondaryTextColor">#ffffff</color>

    <!-- For colored text; default is black/dark-gray (Day) or white/light-gray (Night) -->
    <color name="primaryTextColoredColor">@color/primaryDarkColor</color>
    <color name="SecondaryTextColor">@color/primaryColor</color>

    <!-- Matching colored background -->
    <color name="backgroundColoredColor">@color/primaryLightColor</color>

    <!-- Used for Splash Screen background, and elsewhere as needed -->
    <color name="backgroundDefaultColor">@android:color/background_light</color>
</resources>